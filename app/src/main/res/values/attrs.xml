<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <declare-styleable name="RatioImageView">
        <attr name="ratio" format="float" />
    </declare-styleable>

    <declare-styleable name="NineGridLayout">
        <attr name="sapcing" format="dimension" />
        <attr name="oneImageWidth" format="dimension" />
        <attr name="oneImageHeight" format="dimension" />
        <attr name="image_ratio" format="float" />
    </declare-styleable>

    <declare-styleable name="RatingBar" tools:ignore="ResourceName">
        <attr name="starImageSize" format="dimension" />
        <attr name="starImageWidth" format="dimension" />
        <attr name="starImageHeight" format="dimension" />
        <attr name="starImagePadding" format="dimension" />
        <attr name="starCount" format="integer" />
        <attr name="starEmpty" format="reference" />
        <attr name="starFill" format="reference" />
        <attr name="starHalf" format="reference" />
        <attr name="clickable" format="boolean" />
        <attr name="halfstart" format="boolean" />
    </declare-styleable>
    <declare-styleable name="HexagonImageView">
        <attr name="borders_width" format="dimension" />
        <attr name="borders_color" format="color" />
    </declare-styleable>

    <declare-styleable name="Setting_attr">
        <attr name="leftIcon" format="integer" />
        <attr name="leftText" format="string" />
    </declare-styleable>

    <declare-styleable name="TopChangeView_Attr">
        <attr name="topLeftText" format="string" />
        <attr name="topRightText" format="string" />
    </declare-styleable>
</resources>